<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
	
<hibernate-mapping package="com.example.model">
<class name="SuperVillian" table="Super_Villian">
	<meta attribute="description">
		meta data or Whatever
	</meta>
	<id name="villId" type="int" column="vill_id">
		<generator class="native"></generator>
	</id>
	<property name="name" type="string">
	<column name="name" not-null="true" unique="true"></column>
	</property>
	<property name="superpower" column="superpower" type="string"></property>
	<property name="bounty" column="bounty" type="int"></property>
	


</class>
</hibernate-mapping>	
	